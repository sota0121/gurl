    "version": "0.2.0",
    "configurations": [
        {
            "name": "GET / STDOUT Test",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "-X",
                "GET",
                "https://google.com"
            ]
        },
        {
            "name": "GET / VERBOSE / FILE Test",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "-v",
                "-X",
                "-d",
                "Sample Data In String",
                "--output",
                "google.html",
                "GET",
                "https://google.com",
            ]
        }
    ]
}